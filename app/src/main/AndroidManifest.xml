<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectapp">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the "MyLocation" functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectApp">
        <activity android:name=".RouteAllDetails" />
        <activity android:name=".SavedRoutes" />
        <activity android:name=".CityPrivateTransports" />
        <activity android:name=".CT_TripInfo" />
        <activity android:name=".CT_SearchResults" />
        <activity android:name=".CT_Filters" />
        <activity android:name=".CT_Locations" />
        <activity android:name=".MapSelectLocation" />
        <activity android:name=".ShowTransports" />
        <activity android:name=".taxiprivate" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>